Attribute VB_Name = "twbk"
Attribute VB_Base = "0{00020819-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Option Explicit

Private Sub Workbook_Open()
     Dim strName As String
     
     strName = "ooooooooh picite"
     
     If Hour(Time) >= 0 And Hour(Time) < 12 Then
          MsgBox "Bom dia péque :D", vbInformation, strName
     ElseIf Hour(Time) >= 12 And Hour(Time) < 18 Then
          MsgBox "Boa tarde péque.:D", vbInformation, strName
     Else
          MsgBox "Boa noite péque :D", vbInformation, strName
     End If
     
     strName = vbNullString
End Sub
