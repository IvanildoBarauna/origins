Attribute VB_Name = "frmLançamentos"
Attribute VB_Base = "0{A2B676B9-38C9-41C9-A80E-1EBCA13FF208}{003014BC-195B-4467-85D5-C91932B39991}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = False
Option Explicit

Private Sub btnPrint_Click()
     PrintSheet
End Sub

Private Sub btnSair_Click()
     Unload Me
End Sub

Private Sub txt_databpa_Exit(ByVal Cancel As MSForms.ReturnBoolean)
     Dim ws As Worksheet
     Dim lo As ListObject
     Dim lr As ListRow
     
     Set ws = shDados
     Set lo = ws.ListObjects("DIGITAÇÃO")
     Set lr = lo.ListRows.Add
     
     Me.txt_databpa.Value = Format(Me.txt_databpa.Value, "00\/00\/0000")
     
     With lr
          .Range(1) = Me.cbo_prof.Value
          .Range(2) = Replace(Me.txt_nascto.Value, "/", "")
          .Range(3) = Replace(Me.txt_databpa.Value, "/", "")
     End With
          
     With Me
          .cbo_prof = ""
          .txt_nascto = ""
          .cbo_prof.SetFocus
     End With
     
End Sub

Private Sub txt_nascto_Exit(ByVal Cancel As MSForms.ReturnBoolean)
     Me.txt_nascto.Value = Format(Me.txt_nascto.Value, "00\/00\/0000")
End Sub

Private Sub UserForm_Initialize()
     Dim ws As Worksheet
     Dim lo As ListObject
     Dim lr As Integer
     Dim i As Integer
     
     Set ws = shListas
     Set lo = ws.ListObjects("LISTA_PROCED")
     lr = lo.ListRows.Count + 1
     
     For i = 2 To lr
          cbo_prof.AddItem ws.Cells(i, 1).Value2
     Next i
     
     Me.txt_databpa = DateSerial(Year(Date), Month(Date) - 1, 21)
End Sub
